<?xml version="1.0" encoding="UTF-8"?>
<service>
  <id>GeminiEligibilityCheck</id>
  <name>GeminiEligibilityCheck</name>
  <description>Gemini Eligibility Check Service</description>
  <executable>java</executable>
  <arguments>-jar "%BASE%\GeminiEligibilityCheck.jar" --spring.profiles.active=postgres</arguments>
  <workingdirectory>%BASE%</workingdirectory>
  <log mode="roll-by-size">
    <sizeThreshold>10240</sizeThreshold>
    <keepFiles>10</keepFiles>
  </log>
  <onfailure action="restart" delay="5 sec"/>
  <stopparentprocessfirst>true</stopparentprocessfirst>
</service>
